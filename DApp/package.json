{
  "name": "iexec",
  "version": "1.0.0",
  "description": "",
  "main": "dist/app.js",
  "scripts": {
    "clean": "rimraf build",
    "build": "tsc",
    "start": "node build/src/app.js",
    "test": "jest",
    "lint": "eslint . --ext .ts",
    "docker:build": "docker build . --tag desmo-dapp",
    "onchain:deploy": "iexec app deploy --chain viviani",
    "onchain:run": "iexec app run --watch --chain viviani"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/vaimee/desmo-dapp.git"
  },
  "author": "",
  "license": "ISC",
  "bugs": {
    "url": "https://github.com/vaimee/desmo-dapp/issues"
  },
  "homepage": "https://github.com/vaimee/desmo-dapp#readme",
  "jest": {
    "preset": "ts-jest",
    "moduleFileExtensions": [
      "js",
      "json",
      "ts"
    ],
    "rootDir": "tests",
    "testRegex": ".*\\.test\\.ts$",
    "transform": {
      "^.+\\.(t|j)s$": "ts-jest"
    },
    "collectCoverageFrom": [
      "**/*.(t|j)s"
    ],
    "coverageDirectory": "../coverage",
    "testEnvironment": "node",
    "testTimeout": 500000
  },
  "dependencies": {
    "@node-wot/binding-http": "^0.8.0",
    "@node-wot/core": "^0.8.0",
    "@turf/turf": "^6.5.0",
    "@vaimee/desmold-sdk": "^1.0.0",
    "ajv": "^8.11.0",
    "axios": "^0.26.0",
    "ethers": "^5.6.0",
    "ethers-ts": "^5.5.3",
    "iexec": "^7.2.2",
    "jsonpath": "^1.1.1",
    "ts-jest": "^28.0.6",
    "ts-node": "^10.9.1"
  },
  "devDependencies": {
    "@types/jest": "^28.1.5",
    "eslint": "^8.23.0"
  }
}
